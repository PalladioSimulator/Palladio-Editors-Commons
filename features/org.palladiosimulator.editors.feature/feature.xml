      <import plugin="org.palladiosimulator.pcm.resources" version="4.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.editors.util" version="0.9.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.palladiosimulator.editors.composedprovidingrequiringentity.design"
