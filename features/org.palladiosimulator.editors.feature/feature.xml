      <import plugin="org.eclipse.ui.workbench"/>
      <import plugin="org.eclipse.ui.ide" version="3.10.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.gmf.runtime.diagram.ui" version="1.8.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.sirius.diagram.ui" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.edit.ui" version="2.10.2" match="greaterOrEqual"/>
      <import plugin="org.scaledl.architecturaltemplates" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.mdsdprofiles.api" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.modelversioning.emfprofile.registry" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.scaledl.architecturaltemplates.ui" version="1.0.0" match="greaterOrEqual"/>
