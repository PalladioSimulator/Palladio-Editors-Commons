<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.editors.feature"
      label="Palladio Component Model - Sirius Editors"
      version="1.0.0.qualifier"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="https://sdqweb.ipd.kit.edu/wiki/PCM_Development/Sirius_Editors">
      Sirius is a framework for model-driven graphical editor generation. This feature brings Sirius-based editors to Palladio.
   </description>

   <requires>
